.row
  .col-md-6
    %form.new-user{'ng-submit' => 'fetch()'}
      .form-group
        %input.form-control{type: 'text', 'ng-model' => 'newToken.access_token', placeholder: 'Access Token'}

      .form-group
        %button.btn.btn-primary{type: 'submit'}
          Submit


  .col-md-6{'ng-show' => 'response'}
    .row
      Response: {{responseStatus}}
    .row.json
      {{response}}
